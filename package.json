  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "seed": "node seed.js",
    "dev:css": "tailwindcss -i ./src/styles/tailwind.css -o ./public/css/style.css --watch",
    "build:css": "tailwindcss -i ./src/styles/tailwind.css -o ./public/css/style.css --minify",
    "tw:build": "tailwindcss build -i ./public/css/tailwind.css -o ./public/css/main.css --watch",
    "tw:prod": "tailwindcss build -i ./public/css/tailwind.css -o ./public/css/main.css --minify",
    "dev": "concurrently \"npm run tw:build\" \"node server.js\""
  },
